			<string>func check4(_ array:[String])-&gt;Bool{
    var counter = 1
    var first = array.first
    for i in 1...array.count-1{
        if array[i] == first{
            counter += 1
        }else{
            first = array[i]
            counter = 1
        }
        if counter == 4 &amp;&amp; first != "n" &amp;&amp; first != ""{
            return true
        }
    }
    return false
}

